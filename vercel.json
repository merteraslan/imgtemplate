{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          ".cache/ms-playwright/**" 
        ],
        "maxLambdaSize": "50mb",
        "helpers": false,
        "external": [
          "../node_modules/playwright/node_modules/@playwright/browser-chromium/**"
        ],
        "internal": [
          "../node_modules/playwright/.local-browsers/**"
        ],
        "packageManager": "yum",
        "packages": [
          "alsa-lib",
          "atk",
          "at-spi2-atk",
          "cups-libs",
          "dbus-libs",
          "expat",
          "fontconfig",
          "gcc-c++",
          "gdk-pixbuf2",
          "glib2",
          "glibc",
          "gtk3",
          "libdrm",
          "libgbm",
          "libgcc",
          "libX11",
          "libX11-xcb",
          "libXcomposite",
          "libXcursor",
          "libXdamage",
          "libXext",
          "libXfixes",
          "libXi",
          "libXrandr",
          "libXrender",
          "libXtst",
          "libxkbcommon",
          "libxcb",
          "mesa-libGL",
          "nss",
          "pango",
          "xorg-x11-fonts-100dpi",
          "xorg-x11-fonts-75dpi",
          "xorg-x11-fonts-cyrillic",
          "xorg-x11-fonts-misc",
          "xorg-x11-fonts-Type1",
          "xorg-x11-utils"
        ]
      }
    }
  ]
} 