{
  "version": 2,
  "functions": {
    "api/generate.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/generate-image",
      "dest": "/api/generate.js"
    }
  ],
  "builds": [
    { 
      "src": "api/generate.js", 
      "use": "@vercel/node" 
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ]
} 